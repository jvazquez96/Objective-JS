Class Sum {
	function fib(int x) returns int {		
		if (x <= 2) {
			x = 1;
		} else {
			x = fib(x-1) + fib(x-2);
		}
		return x;
	}
	main() {
		if (true) {
			print("true");
		}
		var i : int;
		i = 0;
		while (i <= 5) {
			print("i", i);
			i++;
		}
		var z : int ; var a : float;
		var x : int;
		print("Give me n for fib");
		read >> x;
		z = fib(x);
		print("fibonnacci", z);
		a = 12;
		print("A: ", a);
	}
}